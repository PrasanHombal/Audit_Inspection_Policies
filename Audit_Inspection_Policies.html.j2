<!DOCTYPE html>
<html>
  <head>
  <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" href="./css_final.css">
    <script src="./filter.js"></script>    
    <title>Audit Inspection Health Rules</title>
  </head>
  <body>
      <div class="heading">
    <h1>Audit Inspection Health Rules</h1>
</div>
<div class="outer-wrapper">
    <div class="table-wrapper">
    <table id="emp-table">
      <thead>
        <tr>
          <th col-index = 1>Environment Name
		    <select class="table-filter" onchange="filter_rows()">
                    <option value="all"></option>
                </select>
				</th>
          <th col-index = 2>Policy. Name
		    <select class="table-filter" onchange="filter_rows()">
                    <option value="all"></option>
                </select></th>
          <th col-index = 3>Enabled
		    <select class="table-filter" onchange="filter_rows()">
                    <option value="all"></option>
                </select>
				</th>
        </tr>
      </thead>
      <tbody>
        {% for item in json_array %}
        <tr>
          <td>{{ item.0.key }}</td>
          <td>{{ item.1.name }}</td>
          <td {% if not item.1.enabled %}class="disabled"{% endif %}>
            {{ item.1.enabled }}
          </td>
        </tr>
        {% endfor %}
      </tbody>
    </table>
	 <script>
        getUniqueValuesFromColumn();
        
    </script>
	</div>
	</div>
  </body>
</html>
